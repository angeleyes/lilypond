#!@FONTFORGE@

Open($1);
MergeKern($2)

# Remove the No. glyph - for want of better FF fix.  
err = SelectIf("afii61352");
if ( err > 0 )
   Cut();
elseif ( err == 0 )
   Print("Could not find \"numero\" glyph");
elseif ( err < 0 )
   Print("An error occurred while searching for the \"numero\" glyph");
endif

Generate($3 + $fontname + ".otf");

