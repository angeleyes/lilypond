  {
%   \override Staff.Custos #'neutral-position = #4
    \override Staff.Custos #'neutral-direction = #-1
    \override Staff.Custos #'style = #'hufnagel
    c'1	\break
    <d' a' f''>1
  }

  \layout {
    \context {
      \Staff
      \consists Custos_engraver
    }
    raggedright = ##t
  }
}
@end lilypond
  
