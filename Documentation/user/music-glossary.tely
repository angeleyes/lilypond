      g4_"mordent" a b\mordent a \bar "||"
      e'4_"prall" d\prall c2 \bar "||"
    }
  }
  \relative c'' {
    a4 c16[ b a b] c2
    g4 a b16-[ a b8] a4
    e'4 e32[ d e d ~ d8] c2
  }
>>
@end lilypond

